#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
        int tam=0;
        for(int i=0;i<n/2;i++){
            if(a[i]!=a[n-i-1]){
                cout<<"NO\n";
                 tam++;
                break;

            }
        }
        if(tam==0) cout<<"YES"<<endl;

    }
    return 0;
}



